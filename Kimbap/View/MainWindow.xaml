<Window x:Class="Kimbap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kimbap"
        xmlns:control="clr-namespace:Kimbap.Control"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <control:TableControl x:Name="tableControl" Visibility="Visible"/>
        <control:MenuControl x:Name="menuControl" Visibility="Collapsed"/>
    </Grid>
</Window>
